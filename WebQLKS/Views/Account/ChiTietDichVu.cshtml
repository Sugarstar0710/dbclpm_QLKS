@model WebQLKS.Models.tbl_DichVuDaDat

@{
    ViewBag.Title = "ChiTietDichVu";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container w-50 text-center mt-3 card">
    <h2>Chi Tiết Dịch Vụ</h2>

    <div>
        <h4>@Model.tbl_DichVu.TenDV</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                Thời Gian:
            </dt>

            <dd>
                @Html.DisplayFor(model => model.NgaySuDungDV)
            </dd>

            <dt>
                Trạng Thái:
            </dt>

            <dd>
                @Html.DisplayFor(model => model.tbl_TrangThaiDichVu.TenTrangThai)
            </dd>

            <dt>
                Mã Khách Hàng:
            </dt>

            <dd>
                @Html.DisplayFor(model => model.tbl_HoaDon.MaKH)
            </dd>

            <dt>
                Họ Tên Khách Hàng
            </dt>

            <dd>
                @Html.DisplayFor(model => model.tbl_KhachHang.HoTen)
            </dd>

            <dt>
                Họ Tên Nhân Viên
            </dt>

            <dd>
                @Html.DisplayFor(model => model.tbl_NhanVien.HoTen)
            </dd>

        </dl>
    </div>
    <p>
        <button type="button" class="btn btn-outline-warning" onclick="history.back();">Quay lại</button>
    </p>
</div>
